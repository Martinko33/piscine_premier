{# je cree ma site de la base quelle je envoye ou y aura demande  #}

<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {# lien pour css #}
    <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    {# je change title par raporte de page souhaite #}
    <title>{% block title %} Je pense que j'ai oubli√© blocke mon title :D {% endblock %}</title>
</head>
<body>
    <header class="header">
        <div class="logo">
            <img src="https://st.depositphotos.com/2400497/2931/v/600/depositphotos_29310837-stock-illustration-trash-can.jpg" alt="logo" class="img">
        </div>
        <ul class="list_menu">
            <li>Articles</li>
            <li>Inscriptions</li>
            <li>Menu</li>
        </ul>
    </header>
{# ici je change block  main qui je rappelle ou je repli par autre page quelle demande block main  #}
    <main>

        <div>
            {% for message in app.flashes('success') %}
                <div class="flash-notice">
                    {{ message }}
                </div>
            {% endfor %}

        </div>
        {% block main %}{% endblock %}

    </main>

    <footer class="footer">
        <ul class="list_footer">
            <li>Adresse</li>
            <li>FAQ</li>
            <li>Contact</li>
        </ul>
        <div>
            <img src="data:image/jpeg;base64," alt="reseaux" class="image">
        </div>


    </footer>

</body>
</html>
